<h1>PRODUCTOS</h1>

<table>
  <thead>
    <tr>
      <th>SKU</th>
      <th>Cantidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dicc_skus.each do |lib| %>
      <tr>
        <td><%= lib[0] %></td>
        <td><%= lib[1] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h1>ALMACENES</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Usado</th>
      <th>Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lista_almacenes.each do |lib| %>
      <tr>
        <td><%= lib[0] %></td>
        <td><%= lib[1] %></td>
        <td><%= lib[2] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<h1>ORDENES DE FABRICACION</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>SKU</th>
      <th>CANTIDAD</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ordenes_fabricacion.each do |lib| %>
      <tr>
        <td><%= lib.id %></td>
        <td><%= lib.sku %></td>
        <td><%= lib.cantidad %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
